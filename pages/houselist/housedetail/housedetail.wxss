/* pages/housedetail/housedetail.wxss */
.wd-swiper {
  width:100%;height:200px;
}
.wd-swiper-item {
  width:100%;height:220px;
}
.title-address {
  padding-left:20px;padding-right:20px;padding-top:10px;padding-bottom:10px;
}
.title-address-text {
  font-weight:bold;font-size:15px;
}
.margin-lr-10 {
  margin-left:10px;margin-right:10px;
}
.title-detail-view {
  background-color:#eeeeee;padding-left:10px;padding-top:6px;padding-bottom:6px;
}
.margin-tb-10 {
  margin-top:10px;margin-bottom:10px;
}
.detail-text {
  font-size:13px;color:#6d6d6d;margin-left:10px;margin-right:10px;
}
.location-icon {
  width:15px;height:15px;
}
.wd-30px {
  width:30px;height:30px;
}
.right-icon-text {
  font-size:10px;color:#8a8a8a;margin-top:2px;
}
.bottom-dial {
  position: fixed;bottom: 0;width: 100%;height:50px;display: flex;flex-direction: row; 
}
.bottom-dial-image {
  width:50%;height:100%;background-color:#E5E5E5;
}
.bottom-dial-btn {
  width:50%;height:100%;background-color:#FFE3B6;flex-direction:column;display:flex;justify-content:center;align-items:center;
}